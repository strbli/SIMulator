flash: $(BUILD_DIR)/$(TARGET).bin
	printf "loadbin $(BUILD_DIR)/$(TARGET).bin 0x8000000\nr\ng\nexit\n" | JLinkExe -device STM32F103C8 -if swd -speed 4000 -autoconnect 1
	
